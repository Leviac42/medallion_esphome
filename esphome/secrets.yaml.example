api_encryption_key: "GENERATE_A_32_BYTE_BASE64_KEY"

# OTA password
ota_password: "YOUR_OTA_PASSWORD"

# Web server password
web_password: "admin"

# Backend API upload URL
upload_url: "http://192.168.1.119:8000/upload"
